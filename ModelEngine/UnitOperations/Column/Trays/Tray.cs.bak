            return result;
        }

        public double LLEEnthalpyTest(Temperature T, double[] x)
        {
            return x[0] * 75.6955631003622 * (T.Celsius - 25)
                + x[1] * 153.669998751979 * (T.Celsius - 25)
                + x[2] * 139.662965979802 * (T.Celsius - 25);
        }

