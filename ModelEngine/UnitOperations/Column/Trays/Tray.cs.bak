            if (TrayLiquid != null)
                TrayLiquid.OnRequestParent += new Func<UnitOperation>(delegate { return GetParent(); });
            if (WaterDraw != null)
                WaterDraw.OnRequestParent += new Func<UnitOperation>(delegate { return GetParent(); });
        }

